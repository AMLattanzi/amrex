main.Linux.gfortran.exe inputs.2d.vcycle --rhs_type 1 | tee out.2d.vcycle.pm
main.Linux.gfortran.exe inputs.2d.fcycle --rhs_type 1 | tee out.2d.fcycle.pm
main.Linux.gfortran.exe inputs.3d.vcycle --rhs_type 1 | tee out.3d.vcycle.pm
main.Linux.gfortran.exe inputs.3d.fcycle --rhs_type 1 | tee out.3d.fcycle.pm
main.Linux.gfortran.exe inputs.2d.vcycle --rhs_type 4 | tee out.2d.vcycle.sin
main.Linux.gfortran.exe inputs.2d.fcycle --rhs_type 4 | tee out.2d.fcycle.sin
main.Linux.gfortran.exe inputs.3d.vcycle --rhs_type 4 | tee out.3d.vcycle.sin
main.Linux.gfortran.exe inputs.3d.fcycle --rhs_type 4 | tee out.3d.fcycle.sin
